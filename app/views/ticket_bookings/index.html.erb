<h1>All Ticket Booking</h1>
<% if user_signed_in? %>
  <%= link_to 'List Events', events_path %><br>
  <%= link_to 'Sign Out', destroy_user_session_path, data: { turbo_method: :delete, turbo_confirm: 'Are you sure you want to sign out?' } %>
<% else %>
  <%= link_to 'Sign In', new_user_session_path %>
<% end %>
<% @ticket_bookings.each do |ticket_booking| %>
  <h3><%= ticket_booking.event.name %></h3>
  <p>Email: <%= ticket_booking.user.email %></p>
  <p>Date: <%= ticket_booking.created_at %></p>
  <p>Tickets Left: <%= ticket_booking.tickets %></p>
<% end %>
